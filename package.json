{
    "name": "SeeleWallet",
    "version": "1.0.0",
    "icon": "SeeleWallet_256.icns",
    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package": "electron-packager . SeeleWallet --all --out=../dist/ --overwrite=true --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --icon=SeeleWallet_48.ico",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "SeeleWallet",
        "dmg": {
            "title": "SeeleWallet",
            "icon": "SeeleWallet_256.icns"
        },
        "mac": {
            "asar": true,
            "target": "dmg",
            "icon": "SeeleWallet_256.icns"
        },
        "win": {
            "target": [
                "nsis",
                "zip"
            ],
            "icon": "SeeleWallet_256.ico"
        },
        "extraResources": {
            "from": "./cmd/",
            "to": "../"
        }
    },
    "repository": "https://github.com/seeleteam/SeeleWallet",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "Seeleteam",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^18.3.7",
        "electron-builder": "^20.36.2"
    },
    "dependencies": {
        "bignumber.js": "^8.0.1",
        "bluebird": "^3.5.2",
        "edit-json-file": "^1.2.1",
        "ps-node": "^0.1.6",
        "seeleteam.js": "^1.6.8",
        "shelljs": "^0.8.3",
        "tree-kill": "^1.2.1"
    }
}
